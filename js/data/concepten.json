---
---
{
    "concepten": {
        {% for concept in site.concepten %}
            "{{ concept.label | default: concept.title | downcase }}": "{{ site.url }}{{ concept.url }}"{% if forloop.last == false %},{% endif %}
        {% endfor %}
    },
    "tags": {
        {% for concept in site.concepten %}
            "{{ concept.label | default: concept.title | downcase }}": {{ concept.tags | jsonify }}{% if forloop.last == false %},{% endif %}
        {% endfor %}
    }
}